<!doctype html>
<html>
  <head>
    <title>Audit Log</title>
  </head>
  <body>
    <h1>Audit Log</h1>

    <table border="1">
      <tr>
        <th>Username</th>
        <th>Success</th>
        <th>IP</th>
        <th>Time</th>
      </tr>

      <% audit.forEach(a => { %>
        <tr>
          <td><%= a.username %></td>
          <td><%= a.success ? "Success" : "Failed" %></td>
          <td><%= a.ip_address %></td>
          <td><%= a.attempted_at %></td>
        </tr>
      <% }) %>
    </table>

    <p><a href="/">Back</a></p>
  </body>
</html>
